        "<leader>",
        "c",
      ],
      "commands": [
        "workbench.action.showCommands",
      ]
    },
    {
      "before": [
        "J"
      ],
      "after": [
        "5",
        "j"
      ]
    },
    {
      "before": [
        "q"
      ],
